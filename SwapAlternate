class HelloWorld {
    public static void main(String[] args) {
        int[] array={5,3,6,34,7};
        swapAlternate(array);
    }

    static void swapAlternate(int[] arr){
        for(int i=0;i<arr.length-1;i+=2){
            int temp=arr[i];
            arr[i]=arr[i+1];
            arr[i+1]=temp;
        }
        for(int i=0;i<arr.length;i++)
        System.out.print(arr[i]+", ");
    }
}
