public class Practice
{

    public static void Try(int arr[])
    {
        int size = arr.length;

        for(int i = 0;i < size-1;i+=2)
        {
            int temp = arr[i];
            arr[i] = arr[i+1];
            arr[i+1] = temp;
        }

        for(int k=0;k<size;k++)
        {
            System.out.print(arr[k] + " ");
        }

    }


        public static void main(String args[])
        {
            int arr[] = {1,2,3,4,5,6};

            Try(arr);

        }
}
